package Amazon.in;

import org.openqa.selenium.By;
import org.openqa.selenium.Keys;
import org.openqa.selenium.WebDriver;
import org.openqa.selenium.chrome.ChromeDriver;

public class ProductVideoPage {

	public static void main(String[] args) {

		System.setProperty("webdriver.chrome.driver", "C:\\Users\\AVU845744\\Desktop\\drivers\\chromedriver.exe");
		WebDriver driver = new ChromeDriver();
		
		driver.get("https://www.amazon.in");

		driver.findElement(By.id("twotabsearchtextbox")).sendKeys("KENT Hand Blender", Keys.ENTER);
		driver.findElement(By.xpath("//*[@id='CardInstanceahj0AyJl4hPMqU7ZOwOg_Q']/div[1]/div[2]/div[1]/div/div[2]/a")).click();
		
		
		
		
		
		
		
	}

}
